							<core:Fragment id="_IDEGen_fragment0" fragmentName="fin.cash.bankmaster.manage/fragment/usedBusinessPartnerSmartResponsiveTable" type="XML"/>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
			<ObjectPageSection id="fin.cash.bankmaster.manage.usedCompanySmartResponsiveTableSection"
				title="{i18n>USED_COMPANY_SMART_RESPONSIVE_TABLE_HEADER}">
				<subSections>
					<ObjectPageSubSection id="_IDEGen_objectpagesubsection1" title="">
